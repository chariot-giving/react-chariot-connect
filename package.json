{
  "name": "react-chariot-connect",
  "version": "1.0.6",
  "description": "React hook and component for integrating with Chariot Connect.",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "prepare": "npm run build"
  },
  "keywords": [
    "react",
    "react component",
    "chariot",
    "chariot giving"
  ],
  "author": "Chariot Giving",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^18.0.12",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "tslib": "^2.5.0",
    "typescript": "^4.7.3"
  },
  "peerDependencies": {
    "react": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chariot-giving/react-chariot-connect.git"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "react-script-hook": "^1.7.2"
  }
}